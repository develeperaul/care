
doctype html
html(lang="ru")
  head
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    title Document
  body
    .wrapper
      include ../sections/header/museum.pug
      main
        - var listLink = ['Иске Элмэт','Музей нефти г. Лениногорск','Музей нефти с. Шугурова']
        section.cards-sect
          .container 
            div(class="links ")
              each item in listLink
                a(href="#")= item 
            .cards
              .card
                img(src=require("/assets/images/museum/1qm.png"), alt="")
            
              .card
                img(src=require("/assets/images/museum/2qm.png"), alt="")
              .card
                img(src=require("/assets/images/museum/3qm.png"), alt="")
            
      include ../sections/footer/index.pug


    